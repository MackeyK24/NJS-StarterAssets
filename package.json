{
  "name": "my-starter-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "npm run dev:classic",
    "dev:classic": "PORT=8080 tsx server/classic.ts & sleep 3 && open http://localhost:8080",
    "dev:colyseus": "PORT=8080 tsx server/colyseus.ts & sleep 3 && open http://localhost:8080",    
    "dev:socketio": "PORT=8080 tsx server/socketio.ts & sleep 3 && open http://localhost:8080",
    "build": "next build",
    "start": "npm run start:classic",
    "start:classic": "NODE_ENV=production tsx server/classic.ts",
    "start:colyseus": "NODE_ENV=production tsx server/colyseus.ts",
    "start:socketio": "NODE_ENV=production tsx server/socketio.ts",
    "lint": "eslint",
    "kill-port": "npx kill-port 8080"
  },
  "dependencies": {
    "@babylonjs-toolkit/dlc": "8.36.1100",
    "@babylonjs-toolkit/next": "8.36.1100",
    "@babylonjs/core": "^8.36.1",
    "@babylonjs/gui": "^8.36.1",
    "@babylonjs/havok": "^1.3.10",
    "@babylonjs/inspector": "^8.36.1",
    "@babylonjs/loaders": "^8.36.1",
    "@babylonjs/materials": "^8.36.1",
    "@colyseus/monitor": "^0.16.7",
    "colyseus": "^0.16.5",
    "express": "^5.1.0",
    "jose": "^6.1.2",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/express": "^5.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "globals": "^16.5.0",
    "mime-types": "^2.1.35",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
